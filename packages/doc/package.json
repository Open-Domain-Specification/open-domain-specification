{
	"name": "@open-domain-specification/doc",
	"version": "0.1.8",
	"description": "Open Domain Specification (ODS) documentation generator",
	"author": "Jonathan Turnock <jonathan.turnock@outlook.com>",
	"homepage": "https://docs.open-ds.io/docs/doc/",
	"license": "ISC",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.mjs",
	"repository": {
		"type": "git",
		"url": "https://github.com/Open-Domain-Specification/open-domain-specification.git"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	},
	"files": [
		"src",
		"dist"
	],
	"scripts": {
		"clean": "npx rimraf dist",
		"build": "tsc --project tsconfig.json && tsup src/index.ts --format cjs,esm --dts --sourcemap --out-dir dist",
		"test": "vitest run --config vitest.config.ts",
		"depcheck": "npx depcheck"
	},
	"keywords": [
		"domain-driven-design",
		"ddd",
		"open-domain-specification",
		"ods"
	],
	"dependencies": {
		"@open-domain-specification/core": "^0.1.8",
		"@open-domain-specification/graphviz": "^0.1.8",
		"typescript": "^5.8.3"
	},
	"devDependencies": {
		"@vitest/coverage-v8": "^3.2.4",
		"tsup": "^8.5.0",
		"vitest": "^3.2.4"
	},
	"gitHead": "59fb63d2b6cc0a108951112088a5e30bc5270c95"
}
