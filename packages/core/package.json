{
	"name": "@open-domain-specification/core",
	"version": "0.0.2",
	"description": "> TODO: description",
	"author": "Jonathan Turnock <jonathan.turnock@outlook.com>",
	"homepage": "",
	"license": "ISC",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.mjs",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	},
	"files": [
		"src",
		"dist"
	],
	"scripts": {
		"clean": "npx rimraf dist",
		"prebuild": "typescript-json-schema --required --noExtraProps --refs --ignoreErrors 'src/schema.ts' 'WorkspaceSchema' > dist/workspace.schema.json",
		"build": "tsc --project tsconfig.json && tsup src/index.ts --format cjs,esm --dts --sourcemap --out-dir dist",
		"test": "vitest run --config vitest.config.ts",
		"depcheck": "npx depcheck"
	},
	"keywords": [
		"domain-driven-design",
		"ddd",
		"open-domain-specification",
		"ods"
	],
	"dependencies": {
		"debug": "^4.4.1",
		"object-hash": "^3.0.0",
		"typescript": "^5.9.2"
	},
	"devDependencies": {
		"@types/debug": "^4.1.12",
		"@types/object-hash": "^3.0.6",
		"@vitest/coverage-v8": "^3.2.4",
		"tsup": "^8.5.0",
		"typescript-json-schema": "^0.65.1",
		"vitest": "^3.2.4"
	}
}
